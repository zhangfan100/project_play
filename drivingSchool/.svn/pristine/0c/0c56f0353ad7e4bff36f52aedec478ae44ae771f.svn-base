.arrow a{ 
	
	position: absolute;
	
	z-index: 2;
	display:block;  
	overflow:hidden;
	cursor:pointer; 
	border-style: solid;
	border-width: 1px;
	border-color: rgb( 195, 195, 198 );
	border-radius: 2px;
	background-color: #d9d9e1;
	width: 20px;
	height: 107px;
}
.arrow a:hover{ 
	background-color: #b5b5b6;
}
.arrow .prev{
	left: -25px;
	top: 0px;
	background-position: 5px 44px;
	background-image: url('../images/prev.png');
	background-repeat: no-repeat;
	background-size: 10px 15px;
}
.arrow .next{
	left: 952px;
	top: 0px;
	background-position: 5px 44px;
	background-image: url('../images/next.png');
	background-repeat: no-repeat;
	background-size: 10px 15px;
}
#place_bar>li>ul>li{
	margin:0 4px;
}