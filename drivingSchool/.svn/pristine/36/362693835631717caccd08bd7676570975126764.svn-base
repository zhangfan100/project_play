	margin:0px;
	padding:0px;
}
#cLeft .sub{
	position: relative;
	top: -11px;
	left: 22px;
	width:0;height:0;  
    line-height:0;  
    border-width:5px;  
    border-style:solid;  
    border-color:  transparent transparent #333232 transparent; 
}
#cLeft .select_title .sub{
	 border-color: #333232 transparent transparent transparent;
	 top:12px;
}
#cLeft ul{
	display: none;
	border:1px solid #C6C6C4;
}
#cLeft>li>span{
	background-repeat: no-repeat;
	background-position: 37px 13px;
}
