	padding: 18px 17px 0 22px;
}
.price-manage .top li{
	display: inline-block;
	font-size:16px;
	margin:0px 3px;
}
.price-manage .top a,.price-manage .edit{
	text-decoration: none;
	display: block;
	width:110px;
	height:38px;
	line-height:38px;
	text-align: center;
	background: url("../../img/admin/p11102.gif") repeat-x top;
