	padding-left:20px;
}
.price-manage .edit{
	width:84px;
	height:30px;
	line-height:30px;
	display: inline-block;
	font-size:15px;
	border:none;
	color:#404040;
}
.price-manage .on{
	color:#EE6A03;
}
