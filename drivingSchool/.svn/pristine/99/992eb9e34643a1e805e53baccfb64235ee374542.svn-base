@CHARSET "UTF-8";
.wrap_container {
	height: 1560px;
}
h1{
	font-size: 18px;
	font-family: "Microsoft YaHei";
	color: rgb( 31, 31, 31 );
}
.top_content ul li a{
	 color: rgb( 70, 70, 70 );
}
.breadcrumb>li+li:before {
	color: rgb( 70, 70, 70 );
	content: ">\00a0";
}
.breadcrumb {
	
	background-color: #ffffff;
}
.left_content_one{
	width:300px;
	height:331px;
	border-style: solid;
	border-width: 2px;
	border-color: rgb( 255, 102, 3 );
	background-color: rgb( 255, 255, 255 );
}
.company_profile{
	line-height: 2.5;
	padding-left: 20px;
	background-color: #ff6603;
	height:45px;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	color: rgb( 255, 255, 255 );
}
.left_content_one ul li{
	cursor: pointer;
	text-align: center;
	height:68px;
	height: 59px;
	line-height: 3.5;
	border-top: 1px solid rgb( 198, 196, 196 );
}
.left_content_one ul li:hover{
	background-color: #eeeeee;
}
.left_content_one ul li a{
	font-size: 16px;
	font-family: "Microsoft YaHei";
	color: rgb( 70, 70, 70 );
}
.left_box_two{
	width:300px;
	border-style: solid;
	border-width: 1px;
	border-color: rgb( 198, 196, 196 );
}

.left_content{
	float:left;
}
.right_content{
	float:right;
	width:685px;
	min-height: 1407px;
	border-style: solid;
	border-width: 1px;
	border-color: rgb( 198, 196, 196 );
}
.right_content .title{
	line-height: 2.5;
	padding-left: 30px;
	height:50px;
	border-bottom: 3px solid rgb( 240, 127, 43 );
}
.right_content img{
	/* margin-top: 20px; */
/* margin-right: auto; */
	/* margin-bottom: 30px; */
}
.sign{
	width:300px;
	height:110px;
	text-align: center;
	float: right;
}
.article{
	font-family: 宋体;
	line-height: 2.2;
	width: 600px;
	margin-left: auto;
	margin-right: auto;
}
.left_box_three{
	width:300px;
	height:345px;
	border-style: solid;
	border-width: 1px;
	border-color: rgb( 198, 196, 196 );
}
.left_box_three ul li{
	border-bottom: 1px dotted rgb( 198, 196, 196 );
	height: 86px;
	padding-top: 10px;
	padding-left: 20px;
}
.panel-group .panel {

	border-radius: 0;
}

.panel-heading{
	font-size:18px;
	padding:0;
}

.panel-default>.panel-heading {
	background-color: #fbfbfb;
}
.panel-heading a{
	color:#686767;
}
.panel-heading div{
	padding:18px 55px;
}
.panel-group .panel+.panel {
	margin:0;
}
.panel-group .panel-heading+.panel-collapse .panel-body {
	border:0;
	text-align:left;
}
.panel-body {
	padding: 15px 0 15px 94px;
	cursor:pointer;
}
.two_level_menu{
	text-align:center;
	background-color:#eeeeee;
	font-size:16px;
}

.panel-body_active{
	background-color:#c8c8c8;
}
.left_box_three_ul li a{
 	font-size: 22px;
    font-family: "Microsoft YaHei";
    color: rgb( 97, 96, 96 );
    padding-left: 10px;
}
.left_box_three_ul li:hover a{
    color: rgb( 240, 127, 43 );}
}

.content > ul {
	list-style: none;
}
.content .menu-one > li {
	width: 296px; 
	height: auto; 
	overflow: hidden; 
	border-top: 1px solid #d2d2d2;
}
.content .menu-one > li.firstChild {
	border: 0;
}
.content .menu-one .header {
	height: 60px; 
	margin:0;
	background: #fdfdfd; 
	line-height: 60px; 
	text-indent: 50px; 
	cursor: pointer;
	width:296px;
}

.content .menu-one .header > span {
	display: block;
}
.content .menu-one .header .txt {
	float: left; 
	color: #686767;
	font-size:18px;
}
.content .menu-show .header {
	background: #fdfdfd;
}
.content .menu-one .header .arrow {
	float: right; 
	width: 100px; 
	height: 60px; 
	background: url(../images/triangle_down_ico.png) no-repeat center center;
}
.content .menu-show .header .arrow {
	background-image: url(../images/triangle_up_ico.png);
}

.content .menu-two {
	display: none; 
	height: auto;
}
.content .menu-two li {
	height:52px; 
	background: #eeeeee; 
	border-top: 0; 
	line-height: 50px; 
	/* text-indent: 60px; */
}
.content .menu-two li.firstChild {
	border: 0;
}
.content .menu-two li.lastChild {
	height:5px;
}
.content .menu-two li a {
	display: block; 
	color: #464646;
}
/* .content .menu-two li:hover, */
.content .menu-two li.menuTwo-current {
	background: #c8c8c8;
}