@CHARSET "UTF-8";
 ol,ul{list-style:none;margin: 0;padding: 0;}
 a:hover {text-decoration: none;}
 /* 字体大小 */
 .f10{font-size:10px;}
.f12{font-size:12px;}
.f14{font-size:14px;}
.f16{font-size:16px;}
.f18{font-size:18px;}
.f20{font-size:20px;}
.f22{font-size:22px;}
.f24{font-size:24px;}
.f40{font-size: 40px;}
.yahei{font-family:Microsoft YaHei;}
.fb{font-weight: bolder;}
.mt10{margin-top: 10px;}
.mt15{margin-top: 15px;}
.mt20{margin-top: 20px;}
.mt25{margin-top: 25px;}
.ml25{margin-left: 25px;}
.pt10{padding-top: 10px;}
.pt15{padding-top: 15px;}
.pt20{padding-top: 20px;}
.pb10{padding-bottom: 10px;}
.pb15{padding-bottom: 15px;}
.pb20{padding-bottom: 20px;}
/* 字体大小 */
/* 字体颜色 */
.red{color:#e23d11;}
.white{color:rgb( 255, 255, 255 );}
.blue{color: rgb( 0, 102, 204 );}
.grey{color: rgb( 68, 68, 68 );}
.light_grey{color: rgb( 133, 131, 131 );}
.light_green{color:#5cb30e;}
.black{color: #000000;}
.gold{color: rgb( 240, 127, 43 );}
/* 字体颜色 */
/* 字体粗细 */
.bold{font-weight: bold;}
/* 字体粗细 */
p{
	text-indent: 2em; /*em是相对单位，2em即现在一个字大小的两倍*/
}
body{/* background-color: #fdfdfd; */font-family: "Microsoft YaHei";}
/*start 顶部 */
.header{
	width: 1000px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 15px;
}
.wrap_container{
	width: 1000px;
	margin-left: auto;
	margin-right: auto;
	margin-top:20px;
}

.sitemap_ul{
	float: right;
	margin-top: 10px;
}
.sitemap_ul li{
	
	float: left;
	/* height: 26px; */
	overflow: hidden;
}
.sitemap_li{
	width: 55px;
}
.sitemap_gap{
	width: 0;
	margin: 1px 10px 0 5px;
	overflow: hidden;
	border-right: 1px solid rgb( 68, 68, 68 );
}
.sitemap_ul li a{
	color: rgb( 68, 68, 68 );
}
.search_box{
	float: right;
	margin-top: 25px;
}
.search_box input{
	border-width: 2px;
	border-color: rgb( 255, 102, 3 );
	border-style: solid;
	background-color: rgb( 255, 255, 255 );
	width: 350px;
  	height: 34px;
  	margin-right: -4px;
}
.search_box button{
	border-width: 2px;
	border-color: rgb( 255, 102, 3 );
	border-style: solid;
	background-color: #ff6600;
	width: 61px;
  	height: 34px;
}
.topnav{
	background-color: #ff6603;
	opacity: 0.988;
	box-shadow: 0px 1px 0px 0px rgb( 195, 78, 2 );
	height: 40px;
}
.topnav ul{
	width: 900px;
	margin-left: auto;
	margin-right: auto;
	height: 40px;
}
.topnav ul li a{
	color: rgb( 255, 255, 255 );
}
.topnav ul li{
	cursor: pointer;
	margin-left: 20px;
	line-height: 2.5;
	text-align:center; 
	float:left;
	width:110px;
}
.topnav ul li:hover{
	background-color: #f29821;
	/* background: url(../images/nav_hover.png)  no-repeat; */
}
.topnav .active{
	background: url(../images/nav_hover.png)  no-repeat;
}
/* end 顶部 */
.footer{
	clear: both;
	height: 300px;
	background-color: rgb( 229, 229, 229 );
}
.footer_center{
	padding-top: 20px;
	width: 1000px;
	margin-left: auto;
	margin-right: auto;
}
.footer_center ul li{
	cursor: pointer;
}
.footer_center h3{
	font-weight: bold;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	color: rgb( 31, 31, 31 );
	
}
.footer_center ul li a{
	line-height: 2;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	color: rgb( 31, 31, 31 );
}
.footer_one,.footer_two,.footer_three,.footer_four{
	float:left;
	margin-right: 60px;
}
.footer_four img{
	margin-left: -10px;
}
.footer_five{
	float:left;
}
