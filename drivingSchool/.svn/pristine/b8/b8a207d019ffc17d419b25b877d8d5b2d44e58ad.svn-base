.header{
	width: 1000px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 15px;
	height: 120px;
	font-family: "Microsoft YaHei";
}
.header_left{
	width: 280px;
	height: 100px;
	float: left;
}
.header_left>img{
	float:left;
}
.header_left span{
	
	display:block;
}
.pingyin {
	padding-left: 95px;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	color: rgb( 23, 23, 23 );
}
.hanzi{
	font-weight: bolder;
	padding-top: 20px;
	font-size: 36px;
	font-family:SimSun;
	color: rgb( 26, 26, 26 );
	
}
.header_right{
	float: right;
	width: 220px;
}
.r1{
height: 52px;
border-left: 1px solid #bfbfbf;
margin: 25px 0 0 20px;
padding: 12px 0 0 40px;
font-size: 18px;
color: #505050;
}
.r1 p a{
font-size: 18px;
color: #505050;
}
.r2{
float: right;
font-size: 18px;
color: #505050;
padding: 40px 0 0;
}
.r2 span{
margin:5px;
}
.r2 span a{
color:#f68125;text-decoration:underline;
}
.form-control {
width: 100%;
height: 38px;
}
.contact_phone {
float:right;
}
.contact_phone ul li{
float:left;
}
.line{
background-color: #ff6603;
opacity: 0.988;
box-shadow: 0px 1px 0px 0px rgb( 195, 78, 2 );
height: 6px;
}
.contact_call{
letter-spacing: 1px;
margin-top: 20px;
margin-left: 25px;
color: rgb( 97, 96, 96 );
line-height: 2.25;
}
.main{
margin:0 auto;

}
.main_content{
width:820px;
height:568px;
margin:0 auto;
}
.col-sm-3 p{
padding: 5px 0 0 0;
font-size: 14px;
color: #afafaf;
}
.col-sm-3{
font-size:16px;
color:#393737;
padding: 5px 0 0 20px;
}
.radio{
padding:1px;
}
.button{
margin:0 auto;
border-bottom:1px solid #bfbfbf;
}
.button_image{
width:1296px;
height:180px;
margin:30px auto 0;
background:url(../images/background_r.png);
overflow:hidden;
text-align:center;
}
input[type="radio"] {
margin: 4px 4px 0;
line-height: normal;
}
.control-label{
font-size:16px;
color:#505050;
padding:10px 0 0;
}
.foot{
margin:0 auto;
/* height:150px; */
}
.foot_content{
width:1000px;
/* height:142px; */
margin:0 auto;
padding:20px;
}
.icon1{
position:relative;
top:-200px;
z-index:-1;
left:-200px;
overflow:hidden;
}
.icon2{
position:relative;
top:70px;
z-index:1;
left:0px;
overflow:hidden;
}
.login_table{
position:relative;
top:-620px;
z-index:2;
left:650px;
background:#fff;
overflow:hidden;
}
.login_table{
width:360px;
height:400px;
border:1px solid #cccaca;
border-radius:3px;
padding:10px;
}
.text{
float:left;
width:25%;
font-size:16px;
color:#606060;
padding:8px 4px;
}
.form-group{
margin:20px 30px;
}
.form-control{
width:100%;
height:38px;
}
.mt{
font-size:18px;
color:#a7a4a4;
margin:0 5px;
padding:0 20px;
}
.l_title{
padding:30px 30px 10px 30px;
}
.l_button{
margin:20px 20px 10px 20px;
text-align:center;
}
.mt1{
float: left;
padding: 0 30px;
font-size: 14px;
color: #848282;
}
.mt2{
float: right;
padding: 0 30px;
font-size: 14px;
color: #848282;
}
.mt1 a,.mt2 a{
color: #848282;
}
.mt1 a:hover,.mt2 a:hover{
color: #f07d22;
}
.footul{

}
.footul li{float:left;list-style: none;}
.foot_li{
width:70px;
}
.foot_li a{
color:#848282;
font-size:14px;
}
.foot_li a:hover{
color:#ee7b21;
font-size:14px;
}
.m3{
color:#848282;
font-size:14px;
}
.foot_gap {
width: 0;
margin: 1px 12px 0 5px;
overflow: hidden;
border-right: 1px solid rgb( 68, 68, 68 );
}
.lfooter{
margin:20px;
}