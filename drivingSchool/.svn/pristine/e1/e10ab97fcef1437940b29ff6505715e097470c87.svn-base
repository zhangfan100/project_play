body,div,ul,li,p,input,button,h3,h2,dd,dl,ol{
    margin:0;
    padding:0;
}
li{list-style: none}
a{
    text-decoration: none;
}
/********************************清除浮动**********************/
.clearfix {
    *zoom: 1;
}
.clearfix:before,
.clearfix:after {
    display: table;
    line-height: 0;
    content: "";
}
.clearfix:after {
    clear: both;
}
/********************************清除浮动 end**********************/
